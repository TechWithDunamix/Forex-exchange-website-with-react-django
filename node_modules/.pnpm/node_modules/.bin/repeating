#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/techwithdunamix/Documents/myprojects/Forex-exchange-website-with-react-django/node_modules/.pnpm/repeating@1.1.3/node_modules/repeating/node_modules:/home/techwithdunamix/Documents/myprojects/Forex-exchange-website-with-react-django/node_modules/.pnpm/repeating@1.1.3/node_modules:/home/techwithdunamix/Documents/myprojects/Forex-exchange-website-with-react-django/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/techwithdunamix/Documents/myprojects/Forex-exchange-website-with-react-django/node_modules/.pnpm/repeating@1.1.3/node_modules/repeating/node_modules:/home/techwithdunamix/Documents/myprojects/Forex-exchange-website-with-react-django/node_modules/.pnpm/repeating@1.1.3/node_modules:/home/techwithdunamix/Documents/myprojects/Forex-exchange-website-with-react-django/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../repeating/cli.js" "$@"
else
  exec node  "$basedir/../repeating/cli.js" "$@"
fi
