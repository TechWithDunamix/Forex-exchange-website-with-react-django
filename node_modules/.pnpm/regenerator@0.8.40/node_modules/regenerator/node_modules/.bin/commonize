#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/techwithdunamix/Documents/myprojects/Forex-exchange-website-with-react-django/node_modules/.pnpm/commoner@0.10.8/node_modules/commoner/bin/node_modules:/home/techwithdunamix/Documents/myprojects/Forex-exchange-website-with-react-django/node_modules/.pnpm/commoner@0.10.8/node_modules/commoner/node_modules:/home/techwithdunamix/Documents/myprojects/Forex-exchange-website-with-react-django/node_modules/.pnpm/commoner@0.10.8/node_modules:/home/techwithdunamix/Documents/myprojects/Forex-exchange-website-with-react-django/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/techwithdunamix/Documents/myprojects/Forex-exchange-website-with-react-django/node_modules/.pnpm/commoner@0.10.8/node_modules/commoner/bin/node_modules:/home/techwithdunamix/Documents/myprojects/Forex-exchange-website-with-react-django/node_modules/.pnpm/commoner@0.10.8/node_modules/commoner/node_modules:/home/techwithdunamix/Documents/myprojects/Forex-exchange-website-with-react-django/node_modules/.pnpm/commoner@0.10.8/node_modules:/home/techwithdunamix/Documents/myprojects/Forex-exchange-website-with-react-django/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../commoner@0.10.8/node_modules/commoner/bin/commonize" "$@"
else
  exec node  "$basedir/../../../../../commoner@0.10.8/node_modules/commoner/bin/commonize" "$@"
fi
